{
  "internationalization": {
    "locales": [
      "zh-Hans",
      "en",
      "ja"
    ],
    "requiredLocales": [
      "zh-Hans",
      "en",
      "ja"
    ],
    "strictMode": "inclusive",
    "defaultLocale": "zh-Hans"
  },
  "middleware": {
    "headerName": "x-intlayer-locale",
    "cookieName": "intlayer-locale",
    "prefixDefault": false,
    "basePath": "",
    "serverSetCookie": "always",
    "noPrefix": false,
    "detectLocaleOnPrefetchNoPrefix": false
  },
  "content": {
    "fileExtensions": [
      ".content.ts",
      ".content.tsx"
    ],
    "baseDir": ".",
    "excludedPath": [
      "node_modules",
      ".intlayer",
      ".next",
      ".nuxt",
      ".expo",
      "dist"
    ],
    "watch": false,
    "contentDir": [
      "src"
    ],
    "dictionariesDir": "src\\locales",
    "moduleAugmentationDir": "src\\types",
    "dictionaryOutput": [
      "intlayer"
    ],
    "unmergedDictionariesDir": ".intlayer\\unmerged_dictionary",
    "dynamicDictionariesDir": ".intlayer\\dynamic_dictionary",
    "i18nextResourcesDir": "i18next_resources",
    "reactIntlMessagesDir": "intl_messages",
    "typesDir": ".intlayer\\types",
    "mainDir": ".intlayer\\main",
    "configDir": ".intlayer\\config",
    "watchedFilesPattern": [
      "/**/*.content.ts",
      "/**/*.content.tsx"
    ],
    "watchedFilesPatternWithPath": [
      "src/**/*.content.ts",
      "src/**/*.content.tsx"
    ],
    "outputFilesPatternWithPath": "src/locales/**/*.json"
  },
  "editor": {
    "applicationURL": "",
    "editorURL": "",
    "cmsURL": "",
    "backendURL": "",
    "port": 8000,
    "enabled": false,
    "dictionaryPriorityStrategy": "local_first",
    "hotReload": false
  },
  "log": {
    "mode": "default",
    "prefix": "\u001b[90m[intlayer] \u001b[0m"
  },
  "ai": {
    "provider": "none",
    "apiKey": "",
    "applicationContext": ""
  },
  "build": {
    "optimize": true,
    "importMode": "static",
    "traversePattern": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/*.d.ts"
    ]
  }
}